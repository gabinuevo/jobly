{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/root.json",
    "type": "object",
    "title": "The Root Schema",
    "required": [
      "title",
      "salary",
      "equity",
      "company_handle"
    ],
    "properties": {
      "title": {
        "$id": "#/properties/title",
        "type": "string",
        "title": "The Title Schema",
        "default": "",
        "examples": [
          "Junior Front End Dev"
        ],
        "pattern": "^(.*)$"
      },
      "salary": {
        "$id": "#/properties/salary",
        "type": "number",
        "title": "The Salary Schema",
        "default": 0.0,
        "examples": [
          100000.00
        ]
      },
      "equity": {
        "$id": "#/properties/equity",
        "type": "number",
        "title": "The Equity Schema",
        "default": 0.0,
        "examples": [
          0.02
        ],
        "minimum": 0.0,
        "maximum": 1.0
      },
      "company_handle": {
        "$id": "#/properties/company_handle",
        "type": "string",
        "title": "The Company_handle Schema",
        "default": "",
        "examples": [
          "ABC"
        ],
        "pattern": "^(.*)$"
      }
    }
  }